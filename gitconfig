[color]
	ui = auto
[user]
	name = Piyush Nimbalkar
	email = piyush@portworx.com
[alias]
	pr = pull --rebase
	co = checkout
	st = status
	ll = log --oneline --graph --decorate
	prs = !git stash save && git pull --rebase && git stash pop && echo 'Success!'
	br = branch
	last = log -1 HEAD
	unstage = reset HEAD --
	lol = log --oneline --graph --decorate --all
	cp = cherry-pick
[push]
	default = upstream
